{
  "name": "pns-subgraph",
  "version": "2.0.1",
  "repository": "https://github.com/pnsdomains/pns-subgraph",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "test": "graph test",
    "setup": "yarn codegen && yarn create-local && yarn deploy-local",
    "local:create": "graph create graphprotocol/pns --node http://127.0.0.1:8020",
    "local:deploy": "graph deploy graphprotocol/pns --debug --ipfs http://127.0.0.1:5001 --node http://127.0.0.1:8020/ --version-label 0.0.1",
    "local:remove": "graph remove graphprotocol/pns --node http://127.0.0.1:8020",
    "server:setup": "yarn codegen && yarn server:create-local && yarn server:deploy-local",
    "server:create": "graph create graphprotocol/tpns --node https://graph-node.pulse.domains",
    "server:deploy": "graph deploy graphprotocol/tpns --debug --ipfs https://ipfs.pulse.domains --node https://graph-node.pulse.domains --version-label 1.0.2",
    "server:remove": "graph remove graphprotocol/tpns --node https://graph-node.pulse.domains",
    "build": "graph build",
    "deploy": "graph deploy graphprotocol/tpns --ipfs https://ipfs.pulse.domains --node https://graph-node.pulse.domains"
  },
  "devDependencies": {
    "@ensdomains/content-hash": "^2.5.3",
    "@graphprotocol/graph-cli": "^0.23.2",
    "@graphprotocol/graph-ts": "^0.29.1",
    "assemblyscript": "^0.26.3",
    "matchstick-as": "^0.5.0",
    "typescript": "^4.9.4"
  }
}
