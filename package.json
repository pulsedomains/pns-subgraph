{
  "name": "ens-subgraph",
  "version": "1.0.2",
  "repository": "https://github.com/pnsdomains/ens-subgraph",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "setup": "yarn codegen && yarn create-local && yarn deploy-local",
    "create-local": "graph create graphprotocol/pns --node http://127.0.0.1:8020",
    "deploy-local": "graph deploy graphprotocol/pns --debug --ipfs http://127.0.0.1:5001 --node http://127.0.0.1:8020/ --version-label 0.0.1",
    "remove-local": "graph remove graphprotocol/pns --node http://127.0.0.1:8020",
    "server:setup": "yarn codegen && yarn server:create-local && yarn server:deploy-local",
    "server:create-local": "graph create graphprotocol/pns --node http://34.228.62.113:8020",
    "server:deploy-local": "graph deploy graphprotocol/pns --debug --ipfs http://34.228.62.113:5001 --node http://34.228.62.113:8020/ --version-label 0.0.1",
    "server:remove-local": "graph remove graphprotocol/pns --node http://34.228.62.113:8020",
    "build": "graph build",
    "deploy": "graph deploy ensdomains/ens --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "watch-local": "graph deploy graphprotocol/ens --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@ensdomains/content-hash": "^2.5.3",
    "@graphprotocol/graph-cli": "^0.23.2",
    "@graphprotocol/graph-ts": "^0.23.1"
  }
}
